    "serverless-plugin-typescript": "^2.1.4",
    "serverless-prune-plugin": "^2.0.1",
    "ts-node": "^10.9.1",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.110",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "mocha": "^10.2.0"
